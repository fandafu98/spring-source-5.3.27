<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="computer" class="com.newcode.createBean.domain.Computer">
		<property name="cpuModel" value="i5-12600KF"/>
		<property name="gpuModel" value="RTX4060"/>
	</bean>

	<bean id="companyBoss" class="com.newcode.createBean.domain.CompanyBoss">
		<property name="bossName" value="马化腾"/>
	</bean>

	<bean id="companyBossBDRPP" class="com.newcode.createBean.bfpp.CompanyBossBeanDefinitionRegistryPostProcessor">

	</bean>

	<bean id="computerBFPP" class="com.newcode.createBean.bfpp.ComputerBeanFactoryPostProcessor">

	</bean>

</beans>